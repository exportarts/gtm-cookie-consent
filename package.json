    "compile-scss": "node-sass projects/ngx-gtm-cookie-consent/src/static/scss/main.scss -o dist/ngx-gtm-cookie-consent/static/css --output-style compressed",
    "copy-readme-and-docs": "cp README.md ./dist/ngx-gtm-cookie-consent && cp -R ./docs ./dist/ngx-gtm-cookie-consent/",
    "test": "echo \"No tests specified\" && exit 0"
